{
  "manifest_version": 2,
  "name": "GuardianAngel",
  "description": "Hide Guardian articles.",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.theguardian.com/*",
        "http://www.theguardian.co.uk/*"
      ],
      "js": [
        "js/jquery-1.10.2.min.js",
        "js/angel.js"
      ]
    }
  ]
}
